{
  "python.pythonPath": "python3",
  "python.linting.flake8Enabled": true,
  "suggest.minTriggerInputLength": 1,
  "suggest.autoTrigger": "always",
  "suggest.noselect": true,
  "suggest.preferCompleteThanJumpPlaceholder": false,
  "suggest.echodocSupport": true,
  "diagnostic.displayByAle": false,
  "diagnostic.enable": false,
  "coc.preferences.promptInput": false,
  "coc.preferences.useQuickfixForLocations": true,
  "css.validate": false,
  "snippets.extends": {
    "javascriptreact": ["javascript", "javascript-react", "javascript_react"],
    "typescriptreact": ["typescript", "javascript", "typescript-react"],
    "typescript": ["javascript"]
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "languageserver": {
    "rust": {
      "command": "rls",
      "args": [],
      "rootPatterns": ["Cargo.toml"],
      "filetypes": ["rust"]
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": ["*.cabal", "stack.yaml", "cabal.project", "package.yaml", "hie.yaml"],
      "filetypes": ["haskell", "lhaskell"]
    },
    "racket": {
      "command": "racket",
      "args": ["-l", "racket-langserver"],
      "filetypes": ["scheme.racket"],
      "trace.server": "verbose",
      "initializationOptions": {
      }
    },
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "clojure-lsp"],
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": {
      }
    },
    "purescript": {
      "command": "purescript-language-server",
      "args": ["--stdio"],
      "filetypes": ["purescript"],
      "trace.server": "off",
      "rootPatterns": ["bower.json", "psc-package.json", "spago.dhall"],
      "settings": {
        "purescript": {
          "addSpagoSources": true
        }
      }
    }
  }
}
