{
    "workbench.editor.navigationScope": "editorGroup",
    "csharp.suppressHiddenDiagnostics": true,
    "[html]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript][typescript][javascriptreact][typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "haskell.formattingProvider": "ormolu",
    "[json][jsonc]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[shellscript]": {
      "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "command-runner.commands": {
      "cd fwd": "cd '${fileDirname}'",
      "nvim-config": "code ~/.config/nvim/init-vsc.vim",
      "rgf": "rgf; exit",
      "tig blame": "tig blame '${file}'"
    },
    "htmlhint.enable": false,
    "markdownlint.config": { "default": false },
    "editor.codeActionsOnSave": {
      "source.fixAll.prettier": true,
      "source.fixAll.eslint": true
    },
    "markdown-preview-enhanced.usePandocParser": true,
    "editor.cursorBlinking": "solid",
    "editor.fontSize": 14,
    "terminal.integrated.fontSize": 14,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.formatOnType": false,
    "editor.wordWrap": "on",
    "files.autoSave": "onWindowChange",
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.currentLine.enabled": false,
    "prettier.requireConfig": true,
    "settingsSync.ignoredSettings": ["workbench.colorCustomizations"],
    "settingsSync.keybindingsPerPlatform": false,
    "telemetry.telemetryLevel": "off",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.macOptionIsMeta": true,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.disableExtension": true,
    // vim
    "vim.insertModeKeyBindings": [
      {
        "after": ["<esc>"],
        "before": ["j", "j"]
      }
    ],
    "vim.smartcase": true,
    "vim.sneak": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "vim.visualstar": true,
    // light vim
    "vim.startInInsertMode": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": false,
    "vim.mouseSelectionGoesIntoVisualMode": false,
