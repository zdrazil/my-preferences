    "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
    "vscode-neovim.neovimExecutablePaths.linux": "/home/linuxbrew/.linuxbrew/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init-vsc.vim",
    "vscode-neovim.neovimInitVimPaths.linux": "~/.config/nvim/init-vsc.vim",
    "vspacecode.bindingOverrides": [
      {
        "keys": "x.s",
        "name": "Shift...",
        "type": "bindings",
        "bindings": [
          {
            "command": "shiftArgumentRight",
            "key": ">",
            "name": "Argument right",
            "type": "command"
          },
          {
            "command": "shiftArgumentLeft",
            "key": "<",
            "name": "Shift argument left",
            "type": "command"
          }
        ]
      },
      {
        "keys": "s.f",
        "name": "fuzzy ripgrep",
        "type": "command",
        "command": "command-runner.run",
        "args": { "command": "rgf" }
      }
    ],
    "workbench.colorCustomizations": {
      "[Default Dark+]": { "editor.background": "#141A21" }
    },
    "workbench.editor.showTabs": false,
    "workbench.list.automaticKeyboardNavigation": false,
    "workbench.preferredDarkColorTheme": "Default Dark+",
    "workbench.preferredLightColorTheme": "Default Light+",
    "zenMode.centerLayout": false,
    "zenMode.fullScreen": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.hideStatusBar": false,
    "zenMode.hideTabs": true,
    "zenMode.restore": true,
    "[markdown]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.wordWrap": "bounded"
    },
    "github.copilot.enable": {
      "*": true,
      "yaml": false,
      "plaintext": true,
      "markdown": false,
      "jsonc": false,
      "typescript": true
    },
    "workbench.settings.editor": "json",
    "editor.inlineSuggest.enabled": true,
    "editor.renderWhitespace": "none",
    "FSharp.suggestGitignore": false,
    "workbench.editor.revealIfOpen": true,
    "window.autoDetectColorScheme": true,
    "redhat.telemetry.enabled": false,
    "liveshare.presence": true,
    "editor.accessibilitySupport": "off",
    "eslint.enable": true
  }